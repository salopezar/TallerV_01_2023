void clearGlobal(void){
	variableBooleana = false;
	parametro1 = 0;
	parametro2 = 0;
}

uint8_t getMaxChar(void){

	uint8_t maxChar = pow(2,8) - 1;

	return maxChar;
}

uint16_t getMaxValue(uint16_t x, uint16_t y, uint16_t z){

	if((x > y) && (x < z)){
		return x;

	}else if((y >= x) && (y >= z)){
		return y;

	}else{
		return z;

	}
}


